{
    "name": "lampager/lampager-cakephp2",
    "description": "Rapid pagination for CakePHP 2",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        },
        {
            "name": "Chitoku",
            "email": "odango@chitoku.jp"
        }
    ],
    "keywords": ["cakephp", "pagination", "paginator", "offset", "limit"],
    "require": {
        "php": "^5.6 || ^7.0",
        "composer/installers": "*",
        "lampager/lampager": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0.0",
        "cakephp/cakephp": "^2.10.4",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "scripts": {
        "test": "php -dzend_extension=xdebug.so vendor/cakephp/cakephp/lib/Cake/Console/cake.php -working `pwd` test app All --coverage-html coverage"
    }
}
